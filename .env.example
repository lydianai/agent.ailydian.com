# HealthCare-AI-Quantum-System Environment Variables
# COPY THIS FILE TO .env AND FILL IN YOUR VALUES
# NEVER COMMIT .env TO VERSION CONTROL

# ============================================================================
# APPLICATION
# ============================================================================
APP_ENV=development
APP_NAME=HealthCare-AI-Quantum-System
APP_VERSION=1.0.0
DEBUG=true
LOG_LEVEL=INFO

# ============================================================================
# API CONFIGURATION
# ============================================================================
API_HOST=0.0.0.0
API_PORT=8080
API_WORKERS=4
API_RELOAD=true

# ============================================================================
# DATABASE - PostgreSQL (Primary)
# ============================================================================
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=healthcare_ai
POSTGRES_USER=healthcare_admin
POSTGRES_PASSWORD=change_this_secure_password_123!
POSTGRES_SCHEMA=public

# Connection Pool
POSTGRES_POOL_SIZE=20
POSTGRES_MAX_OVERFLOW=10

# ============================================================================
# DATABASE - MongoDB (Documents & Images)
# ============================================================================
MONGODB_HOST=localhost
MONGODB_PORT=27017
MONGODB_DB=healthcare_ai_docs
MONGODB_USER=healthcare_admin
MONGODB_PASSWORD=change_this_secure_password_123!

# ============================================================================
# DATABASE - TimescaleDB (Time-Series)
# ============================================================================
TIMESCALE_HOST=localhost
TIMESCALE_PORT=5433
TIMESCALE_DB=healthcare_ai_timeseries
TIMESCALE_USER=healthcare_admin
TIMESCALE_PASSWORD=change_this_secure_password_123!

# ============================================================================
# CACHE - Redis
# ============================================================================
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0
REDIS_PASSWORD=
REDIS_CACHE_TTL=300

# ============================================================================
# MESSAGE QUEUE - Kafka
# ============================================================================
KAFKA_BOOTSTRAP_SERVERS=localhost:9092
KAFKA_TOPIC_PATIENT_EVENTS=patient-events
KAFKA_TOPIC_AGENT_COMM=agent-communications
KAFKA_TOPIC_ALERTS=patient-alerts
KAFKA_CONSUMER_GROUP=healthcare-ai-agents

# ============================================================================
# MESSAGE QUEUE - RabbitMQ (Alternative)
# ============================================================================
RABBITMQ_HOST=localhost
RABBITMQ_PORT=5672
RABBITMQ_USER=healthcare_admin
RABBITMQ_PASSWORD=change_this_secure_password_123!

# ============================================================================
# AI/ML - OpenAI
# ============================================================================
OPENAI_API_KEY=sk-your-openai-key-here
OPENAI_MODEL_GPT4=gpt-4-turbo-preview
OPENAI_MODEL_GPT4O=gpt-4o
OPENAI_MAX_TOKENS=4000
OPENAI_TEMPERATURE=0.3

# ============================================================================
# AI/ML - Anthropic (Claude)
# ============================================================================
ANTHROPIC_API_KEY=sk-ant-your-anthropic-key-here
ANTHROPIC_MODEL=claude-3-opus-20240229
ANTHROPIC_MAX_TOKENS=4000
ANTHROPIC_TEMPERATURE=0.3

# ============================================================================
# AI/ML - Google (Gemini)
# ============================================================================
GOOGLE_API_KEY=your-google-api-key-here
GOOGLE_MODEL=gemini-pro

# ============================================================================
# QUANTUM COMPUTING - IBM Quantum
# ============================================================================
IBM_QUANTUM_TOKEN=your-ibm-quantum-token-here
IBM_QUANTUM_BACKEND=ibm_brisbane
IBM_QUANTUM_HUB=ibm-q
IBM_QUANTUM_GROUP=open
IBM_QUANTUM_PROJECT=main

# ============================================================================
# CLOUD - AWS (for HIPAA-compliant storage)
# ============================================================================
AWS_ACCESS_KEY_ID=your-aws-access-key
AWS_SECRET_ACCESS_KEY=your-aws-secret-key
AWS_REGION=us-east-1
AWS_S3_BUCKET_MEDICAL_IMAGES=healthcare-ai-medical-images
AWS_S3_BUCKET_PHI_ENCRYPTED=healthcare-ai-phi-encrypted

# ============================================================================
# SECURITY - Encryption
# ============================================================================
# Generate with: openssl rand -hex 32
SECRET_KEY=your-secret-key-for-jwt-signing-change-this
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# Encryption for PHI
PHI_ENCRYPTION_KEY=your-aes-256-encryption-key-change-this
ENCRYPTION_KEY_VERSION=1

# ============================================================================
# SECURITY - Authentication
# ============================================================================
AUTH_ENABLED=true
OAUTH2_PROVIDER=okta
OAUTH2_CLIENT_ID=your-oauth-client-id
OAUTH2_CLIENT_SECRET=your-oauth-client-secret
OAUTH2_DOMAIN=your-domain.okta.com

# ============================================================================
# COMPLIANCE - HIPAA
# ============================================================================
HIPAA_MODE=true
AUDIT_LOGGING_ENABLED=true
PHI_ENCRYPTION_REQUIRED=true
MIN_PASSWORD_LENGTH=12
MFA_REQUIRED=true

# ============================================================================
# COMPLIANCE - KVKK (Turkey)
# ============================================================================
KVKK_MODE=true
DATA_RETENTION_DAYS=2555  # 7 years
DATA_LOCALIZATION_TURKEY=false

# ============================================================================
# MONITORING - Prometheus
# ============================================================================
PROMETHEUS_ENABLED=true
PROMETHEUS_PORT=9090

# ============================================================================
# MONITORING - Sentry (Error Tracking)
# ============================================================================
SENTRY_DSN=your-sentry-dsn-here
SENTRY_ENVIRONMENT=development
SENTRY_TRACES_SAMPLE_RATE=0.1

# ============================================================================
# FEATURE FLAGS
# ============================================================================
ENABLE_QUANTUM_OPTIMIZATION=false  # Set true when IBM Quantum configured
ENABLE_CLINICAL_DECISION_AGENT=true
ENABLE_RESOURCE_OPTIMIZATION_AGENT=true
ENABLE_PATIENT_MONITORING_AGENT=true
ENABLE_EMERGENCY_RESPONSE_AGENT=false
ENABLE_DIAGNOSIS_AGENT=false
ENABLE_TREATMENT_PLANNING_AGENT=false
ENABLE_PHARMACY_MANAGEMENT_AGENT=false

# ============================================================================
# AGENT CONFIGURATION
# ============================================================================
AGENT_DECISION_TIMEOUT_SECONDS=30
AGENT_CONFIDENCE_THRESHOLD=0.8
AGENT_HUMAN_REVIEW_THRESHOLD=0.7
AGENT_MAX_RETRIES=3

# ============================================================================
# HEALTHCARE INTEGRATION
# ============================================================================
HL7_FHIR_BASE_URL=http://localhost:8081/fhir
HL7_VERSION=2.5
DICOM_AE_TITLE=HEALTHCARE_AI
DICOM_PORT=11112

# ============================================================================
# TESTING
# ============================================================================
TEST_MODE=false
MOCK_LLM_RESPONSES=false
SYNTHETIC_PATIENT_DATA=true
